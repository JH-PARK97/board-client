{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/api/*":["src/api/*"],
        },
        "jsx": "react",
        "allowJs": true,
        "target": "ES6",
        "outDir": "./dist",
        "moduleResolution": "Node",
        "lib": ["ES2015", "DOM", "DOM.Iterable"],
        "esModuleInterop": true,
        "strict": true
    },
    "include": ["./src/**/*"],
    "exclude": ["node_modules", "dist"]
}
